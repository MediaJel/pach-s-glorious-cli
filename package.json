{
  "name": "pach-s-glorious-cli",
  "description": "pach CLI v2 written in nodejs",
  "version": "1.0.0",
  "author": "Pacholo Amit @pacholoamit",
  "bin": {
    "pach": "./bin/run"
  },
  "bugs": "https://github.com/pacholoamit/pach-s-glorious-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@types/node-notifier": "^8.0.1",
    "@types/snowflake-sdk": "^1.6.0",
    "cli-ux": "^5.6.3",
    "eslint-config-prettier": "^8.3.0",
    "node-notifier": "^10.0.0",
    "snowflake-sdk": "^1.6.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/pacholoamit/pach-cli-v2",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "pach",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "pacholoamit/pach-cli-v2",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
